{
  "name": "guideclip",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export",
    "analyze": "ANALYZE=true npm run build",
    "type-check": "tsc --noEmit",
    "maintenance:on": "node scripts/maintenance.js on",
    "maintenance:off": "node scripts/maintenance.js off",
    "maintenance:on:live": "vercel env add MAINTENANCE_MODE true production && vercel --prod",
    "maintenance:off:live": "vercel env add MAINTENANCE_MODE false production && vercel --prod",
    "maintenance:on:api": "node scripts/vercel-maintenance.js on",
    "maintenance:off:api": "node scripts/vercel-maintenance.js off",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=.next",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "preview": "npm run build && npm run start"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "next-auth": "^4.24.11",
    "prisma": "^6.8.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "tailwindcss": "^4",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5"
  }
}
